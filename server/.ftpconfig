{
    "protocol": "sftp",
    "host": "서버주소", // 서버 주소
    "port": 22, // 사용할 포트번호
    "user": "사용자이름", //서버에서 사용하는 user name
    "pass": "", // 비밀번호... 지만 ssh로 public key private key로 비밀번호 없이 통신 세팅을 해 놓았다면 빈 칸으로 하면 된다.
    "promptForPass": false,
    "remote": "서버 폴더경로",  // 서버에 동기화할 폴더경로
    "agent": "",
    "privatekey": "ssh private key 경로", // ssh private key 경로
    "passphrase": "",
    "hosthash": "",
    "ignorehost": true,
    "connTimeout": 10000,
    "keepalive": 10000,
    "watch":[
        "./dist/stylesheets/main.css",
        "./dist/stylesheets/",
        "./dist/stylesheets/*.css"
    ],
    "watchTimeout":500
}
